<h1>User#index </h1>
<p>Find me in app/views/user/index.html.erb</p>

<table>
<thead>
  <tr>
    <th>Name</th>
    <th>Age</th>
    <th>Month of birth</th>
    <th>Year of birth</th>
    <th>Favorite color</th>
  </tr>
</thead>

<tbody>
  <% @users.each do |user| %>
    <tr>
    <td><%= user.name %></td>
    <td><%= user.age %></td>
    <td><%= user.month_of_birth %></td>
    <td><%= user.year_of_birth %></td>
    <td><%= user.favorite_color %></td>
    <td><%= link_to 'Show', show_user_path(user)%></td>
    <td><%= link_to 'Edit', edit_user_path(user)%></td>
    <td><%= link_to 'Delete', delete_user_path(user), method: :delete%></td>
    </tr>
    <% end %>
</tbody>
</table>

<%= link_to 'New User', new_user_path %>
